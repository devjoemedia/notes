import axios from 'axios'

class {

  //Post request for Creating Note
  handleSubmit(data) {

   axios.post('/api/notes',data)
    .then(data => {
      if(data.data.message === "Success") {
        alert("Note Created Please visit / to view note")
      }
    })
    .catch((error) => {
      console.error('Error:', error);
    });

  }

}

<div.container>
  <Header />
  <div.form__inner>
    <h1>Add Note</h1>
    <Form on-set-submit("handleSubmit") />
  </div>
</div>
